      "customMessage": "* %author %message",
      "useCustomMessage": true
    },
    "adminMessage": {
      "customMessage": "[%author: %message]",
      "useCustomMessage": true
    },
    "sayMessage": {
      "customMessage": "[%author] %message",
      "useCustomMessage": true
